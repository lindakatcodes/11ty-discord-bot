{
  "name": "11ty-discord-bot",
  "version": "1.0.0",
  "description": "Workshop repo for training with Dom!",
  "main": "index.js",
  "scripts": {
    "dev:css": "npx tailwindcss -i src/styles.css -o dist/styles.css -w",
    "dev:11ty": "eleventy --config=_eleventy/config.js --serve --watch",
    "dev": "node_modules/.bin/run-p dev:*",
    "build:css": "NODE_ENV=production npx tailwindcss -i src/styles.css -o dist/styles.css -m",
    "build:11ty": "eleventy --config=_eleventy/config.js --output=dist",
    "build": "node_modules/.bin/run-s build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lindakatcodes/11ty-discord-bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lindakatcodes/11ty-discord-bot/issues"
  },
  "homepage": "https://github.com/lindakatcodes/11ty-discord-bot#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@netlify/functions": "^1.2.0",
    "dotenv": "^16.0.1",
    "node-fetch": "2",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.1.8"
  }
}
